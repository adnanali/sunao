<h1>New Content Type</h1>

<% form_for([:admin, @content_type]) do |f| %>

  <p>
    <%= f.label :type %>
    <%= f.text_field :type %>
  </p>
  <p>
    <%= f.label :data %>
    <%= f.text_area :data %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', admin_content_types_path %>